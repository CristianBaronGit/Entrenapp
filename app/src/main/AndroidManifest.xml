<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.entrenapp">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_aplicacion"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Entrenapp"
        tools:targetApi="31">
        <activity
            android:name=".ListaDietas"
            android:exported="false"
            android:label="Dietas"
            android:theme="@style/AppthemeRutina"/>
        <activity
            android:name=".RegistroDietas"
            android:exported="false"
            android:label="Registro Dietas"
            android:theme="@style/AppthemeRutina" />
        <activity
            android:name=".RegistroExamenmedico"
            android:exported="false"
            android:label="Registro Examen Medico y Fisico"
            android:theme="@style/AppthemeRutina" />
        <activity
            android:name=".ListaRutinas"
            android:exported="false"
            android:label="Rutinas"
            android:theme="@style/AppthemeRutina" />
        <activity
            android:name=".RegistroRutina"
            android:exported="false"
            android:label="Registro Rutinas"
            android:theme="@style/AppthemeRutina" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".RegistroUsuarios"
            android:exported="false" />
        <activity
            android:name=".IngresoUsuario"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>